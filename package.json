{
  "name": "vue-boring-avatars",
  "version": "0.1.0",
  "description": "vue-boring-avatars is a Vue.js port of Boring Avatars, a JS library that generates custom, SVG-based, round avatars from any username and color palette.",
  "license": "MIT",
  "homepage": "https://github.com/mujahidfa/vue-boring-avatars",
  "repository": {
    "type": "git",
    "url": "https://github.com/mujahidfa/vue-boring-avatars"
  },
  "bugs": {
    "url": "https://github.com/mujahidfa/vue-boring-avatars/issues"
  },
  "keywords": [
    "vue",
    "vue3",
    "Vue 3",
    "avatar",
    "gravatar",
    "profile picture",
    "user avatar",
    "avatar generator",
    "avatar placeholder"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/vue-boring-avatars.umd.js",
  "module": "./dist/vue-boring-avatars.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-boring-avatars.es.js",
      "require": "./dist/vue-boring-avatars.umd.js"
    }
  },
  "types": "./dist/vue-boring-avatars.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && vue-dts-gen src/components/*.vue && mv src/components/Avatar.d.ts dist/vue-boring-avatars.d.ts && rm -r src/components/Avatar*.d.ts",
    "serve": "vite preview"
  },
  "dependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "typescript": "^4.3.2",
    "vite": "^2.3.4",
    "vue-dts-gen": "^0.2.3",
    "vue-tsc": "^0.0.24"
  }
}
